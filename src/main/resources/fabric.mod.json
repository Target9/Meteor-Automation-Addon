{
  "schemaVersion": 1,
  "id": "meteor-automation",
  "version": "${version}",
  "name": "Meteor Automation",
  "description": "A addon for Meteor Client that allows the bot to handle big tasks",
  "authors": [
    "Target"
  ],
  "contact": {
    "sources": "https://github.com/RacoonDog/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "license": "CC0-1.0",
  "entrypoints": {
    "meteor": [
      "com.meteor.automation.MeteorAutomation"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "meteor-client:commit": "${commit}"
  },
  "depends": {
    "java": ">=21",
    "minecraft": "~1.21",
    "meteor-client": "*"
  }
}
